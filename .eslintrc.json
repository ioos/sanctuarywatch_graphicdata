{
  "root": true,
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "wp": "readonly",
    "jQuery": "readonly",
    "$": "readonly",
    "ajaxurl": "readonly"
  },
  "ignorePatterns": [
    "node_modules/",
    "vendor/",
    "plugins/graphic_data_plugin/admin/exopite-simple-options/**",
    "*.min.js",
    "build/",
    "dist/"
  ],
  "overrides": [
    {
      "files": ["*.php"],
      "processor": "@wordpress/eslint-plugin/php"
    }
  ],
  "rules": {
    "no-console": "off",
    "jsdoc/require-jsdoc": "off",
    "no-unused-vars": "warn"
  }
}
